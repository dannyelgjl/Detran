    "web": {
      "favicon": "./assets/favicon.png"
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.detran.detranpi"
    },
    "android": {
      "package": "com.detran.detran_pi",
      "versionCode": 1
    }
  }
}
